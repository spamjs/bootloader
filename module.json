{
	"name" : "webmodules",
	"*" : {
		"bootconfig": "bootloader.config.js",
		"jquery": "bootloader.config.js"
	}
	"jquery" : {
		js : ["external.jquery",]
	}

	"bootloader_only": {
		"js": [ "bootloader.js"],
		"on": ["webmodules/foo", "webmodules/jquery"]
	},

	"bootloader": {
		"js": ["boot#bootloader.js"],
		"on": ["jqgeeks/bootloader_only", "jqgeeks/bootloader_config"]
	}
}



